{
  "name": "opencage-api-client",
  "version": "0.3.0",
  "description": "An OpenCageData Geocoder API client library for node and browsers (UMD)",
  "main": "dist/opencage-api.node.js",
  "unpkg": "dist/opencage-api.min.js",
  "browser": "dist/opencage-api.min.js",
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "build": "webpack --mode production",
    "codecov": "codecov",
    "coverage": "npm run test && npm run codecov",
    "docs": "npx documentation build src/** -f html -o docs",
    "lint": "eslint .",
    "prebuild": "npm run test",
    "precommit": "lint-staged",
    "pretest": "npm run lint",
    "test": "jest --env=node --coverage",
    "test:watch": "npm test -- --watch"
  },
  "keywords": [
    "geocoder",
    "api",
    "opencage",
    "opencagedata"
  ],
  "author": "Arnaud Ferrand",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.0.4",
    "documentation": "^5.5.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "jest": "^22.4.4",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.2",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "dotenv": "^5.0.1",
    "es6-promise": "^4.2.4",
    "fetch-everywhere": "^1.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsamaya/opencage-api-client.git"
  },
  "bugs": {
    "url": "https://github.com/tsamaya/opencage-api-client/issues"
  },
  "homepage": "https://github.com/tsamaya/opencage-api-client#readme"
}
